<h1 class="text-3xl font-bold mb-6"><%= heading %></h1>
<% for(var i=0;i<data.length;i++) { %>
    <div class="relative w-full max-w-lg bg-white shadow-md rounded-lg p-6 mb-4">
      <div class="flex items-center justify-between mb-4">
        <h2 class="text-lg font-semibold"><%= data[i].name %></h2>
        <% if (data[i].userId == userId) { %>
          <a class="bg-purple-200" href="#" onClick="redirectToEventForm(<%= data[i].id %>)">
            <svg
                  xmlns="http://www.w3.org/2000/svg"
                  fill="none"
                  viewBox="0 0 24 24"
                  stroke-width="2.0"
                  stroke="currentColor"
                  class="w-4 h-4"
                >
                  <path
                    stroke-linecap="round"
                    stroke-linejoin="round"
                    d="M14.74 9l-.346 9m-4.788 0L9.26 9m9.968-3.21c.342.052.682.107 1.022.166m-1.022-.165L18.16 19.673a2.25 2.25 0 01-2.244 2.077H8.084a2.25 2.25 0 01-2.244-2.077L4.772 5.79m14.456 0a48.108 48.108 0 00-3.478-.397m-12 .562c.34-.059.68-.114 1.022-.165m0 0a48.11 48.11 0 013.478-.397m7.5 0v-.916c0-1.18-.91-2.164-2.09-2.201a51.964 51.964 0 00-3.32 0c-1.18.037-2.09 1.022-2.09 2.201v.916m7.5 0a48.667 48.667 0 00-7.5 0"
                  />
                </svg>
          </a>
        <% } %>
      </div>
    <p class="text-gray-700 mb-2"><%= data[i].description %></p>
    <p class="text-gray-700 mb-2">
      Date: <%= new Date(data[i].date).toDateString() %>
    </p>
    <p class="text-gray-700 mb-2 font-semibold">
      Number of Participants: <%= data[i].no_of_participants %>
    </p>
    <p class="text-gray-700 mb-2 font-semibold">
      Event Venue: <%= data[i].venue %>
    </p>
  </div>
<% } %>


